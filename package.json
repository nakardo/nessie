{
  "name": "nessie",
  "version": "0.1.1",
  "description": "An NES emulator written in JavaScript",
  "license": "MIT",
  "author": "Diego Acosta <diegonake@gmail.com>",
  "keywords": [
    "nintendo",
    "nes",
    "emulator"
  ],
  "main": "src/nes.js",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint '**/*.js'",
    "pretty": "prettier --write '**/*.{js,json}'",
    "prepack": "npm run build"
  },
  "dependencies": {
    "canvas": "^2.6.1",
    "debug": "^4.3.1",
    "performance-now": "^2.1.0",
    "process": "^0.11.10",
    "raf": "^3.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-strict-mode": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "gifencoder": "^2.0.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.json": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nakardo/nessie.git"
  },
  "bugs": {
    "url": "https://github.com/nakardo/nessie/issues"
  },
  "homepage": "https://github.com/nakardo/nessie#readme",
  "engines": {
    "node": ">=14"
  },
  "directories": {
    "example": "examples"
  },
  "files": [
    "dist",
    "src"
  ]
}
