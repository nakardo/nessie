{
  "name": "nessie",
  "version": "0.0.4",
  "description": "A NES emulator written in JavaScript",
  "license": "MIT",
  "author": "Diego Acosta <diegonake@gmail.com>",
  "keywords": [
    "nintendo",
    "nes",
    "emulator"
  ],
  "main": "src/nes.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint '**/*.js'",
    "pretty": "prettier --write '**/*.{js,json}'",
    "prepack": "npm run build"
  },
  "dependencies": {
    "canvas": "^2.6.1",
    "debug": "^4.1.1",
    "performance-now": "^2.1.0",
    "raf": "^3.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.0",
    "@babel/core": "^7.10.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-strict-mode": "^7.8.3",
    "@babel/preset-env": "^7.10.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "gifencoder": "^2.0.1",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jquery": "^3.5.1",
    "lint-staged": "^10.2.6",
    "prettier": "2.0.5",
    "terser-webpack-plugin": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.json": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nakardo/nessie.git"
  },
  "bugs": {
    "url": "https://github.com/nakardo/nessie/issues"
  },
  "homepage": "https://github.com/nakardo/nessie#readme",
  "engines": {
    "node": ">=12"
  },
  "directories": {
    "example": "examples"
  },
  "files": [
    "dist",
    "src"
  ]
}
