{
  "name": "node-nes",
  "version": "1.0.0",
  "description": "A NES emulator written in JavaScript",
  "scripts": {
    "lint": "eslint '**/*.js' --ignore-path .gitignore",
    "pretty": "prettier --write '**/*.{js,json}'",
    "start": "babel-node index.js",
    "debug": "nodemon --inspect-brk --exec babel-node index.js",
    "build": "webpack"
  },
  "author": "Diego Acosta <diegonake@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.17.5",
    "prettier": "1.14.2",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "left-pad": "^1.3.0",
    "raf": "^3.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "**/*.json": [
      "prettier --write",
      "git add"
    ]
  }
}
